{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "create date",
          "name": "createdAt",
          "type": "datetime"
        },
        {
          "title": "Last modified date",
          "name": "updatedAt",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "img preview",
          "name": "img",
          "type": "string"
        },
        {
          "title": "img description",
          "name": "imgDescription",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "next",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.preview.host": "http://localhost:3000",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/note/github",
      "title": "github"
    },
    {
      "path": "[[workspace]]/note/react",
      "title": "react"
    },
    {
      "path": "[[workspace]]/note/tool",
      "title": "tool"
    },
    {
      "path": "[[workspace]]/note/week",
      "title": "week"
    },
    {
      "path": "[[workspace]]/note/archived",
      "title": "archived"
    },
    {
      "path": "[[workspace]]/note/handwriting",
      "title": "handwriting"
    },
    {
      "title": "css",
      "path": "[[workspace]]/note/css"
    }
  ],
  "frontMatter.git.enabled": true
}
