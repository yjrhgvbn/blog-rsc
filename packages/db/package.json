{
  "name": "@repo/db",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/index.ts"
      ],
      "types": [
        "./src/types.ts"
      ]
    }
  },
  "scripts": {
    "db:generate": "dotenv -e ../../.env prisma generate",
    "db:push": "dotenv -e ../../.env prisma db push",
    "db:reset": "dotenv -e ../../.env prisma db push -- --force-reset",
    "note": "dotenv -e ../../.env tsx ./scripts/save.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/node": "^20.10.4"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "hasha": "^6.0.0",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0"
  }
}
